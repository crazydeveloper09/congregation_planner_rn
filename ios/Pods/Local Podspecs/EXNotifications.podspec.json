{
  "name": "EXNotifications",
  "version": "0.31.4",
  "summary": "Provides an API to fetch push notification tokens and to present, schedule, receive, and respond to notifications.",
  "description": "Provides an API to fetch push notification tokens and to present, schedule, receive, and respond to notifications.",
  "license": "MIT",
  "authors": "650 Industries, Inc.",
  "homepage": "https://docs.expo.dev/versions/latest/sdk/notifications/",
  "platforms": {
    "ios": "15.1"
  },
  "source": {
    "git": "https://github.com/expo/expo.git"
  },
  "static_framework": true,
  "dependencies": {
    "ExpoModulesCore": []
  },
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES",
    "SWIFT_COMPILATION_MODE": "wholemodule"
  },
  "resource_bundles": {
    "ExpoNotifications_privacy": [
      "PrivacyInfo.xcprivacy"
    ]
  },
  "source_files": "EXNotifications/**/*.{h,m,swift}",
  "exclude_files": "Tests/",
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "dependencies": {
        "ExpoModulesTestCore": []
      },
      "source_files": "Tests/**/*.{m,mm,swift}"
    }
  ]
}
